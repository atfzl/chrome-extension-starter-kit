{
  "manifest_version": 2,
  "name": "Starter Kit",
  "description": "Chrome Extension Starter Kit",
  "version": "1.0",
  "permissions": ["<all_urls>", "activeTab"],
  "web_accessible_resources": ["inject.bundle.js"],
  "background": {
    "scripts": ["hot-reload.js", "background.bundle.js"],
    "persistent": true
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": ["content.bundle.js"]
    }
  ]
}
